{
  "name": "webpack-simple-start",
  "version": "0.1",
  "description": "Simple webpack 5 start template with PostCSS, Lint, Babel, Layers, Bootstrap etc.",
  "main": "index.js",
  "author": "Andrii Plisiuk",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "dev": "cross-env NODE_ENV=development webpack --watch --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint 'src/**/*.js' || true",
    "lint:css": "stylelint 'src/**/*.scss'",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "keywords": [
    "webpack",
    "webpack template",
    "webpack 5",
    "CSS Layers",
    "Babel",
    "Bootstrap",
    "Lint",
    "PostCSS",
    "start webpack template",
    "prettier",
    "ejs loader",
    "ejs",
    "inputmask",
    "swiper slider"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@csstools/postcss-bundler": "^2.0.6",
    "@eslint/compat": "^1.2.5",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.6",
    "ejs": "^3.1.10",
    "ejs-loader": "^0.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.14.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "core-js": "^3.40.0",
    "inputmask": "^5.0.9",
    "swiper": "^11.2.1"
  }
}
